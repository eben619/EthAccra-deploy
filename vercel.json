{
  "rewrites": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "2023.ethaccra.xyz"
        }
      ],
      "destination": "/2023"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.ethaccra.xyz"
        }
      ],
      "destination": "https://ethaccra.xyz/$1",
      "permanent": true
    }
  ]
}